{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 10,
      "maxLength": 100,
      "pattern": ".*[a-zA-Z]+.*"
    },
    "accountid": {
      "type": "integer"
 
    },
    "price": {
      "type": "integer"

    },
    "description": {
      "type": "string"
    },
    "folder": {
      "type": "string"
    },
    "category": {
      "type": "integer"
    }
    
   
  },
  "required": ["name"]
}